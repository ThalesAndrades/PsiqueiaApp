# Configurações do Ambiente - PsiqueiaApp

# Configuração do Expo
EXPO_TOKEN=your_expo_token_here

# Configuração do App Store Connect API
APP_STORE_CONNECT_API_KEY_ID=your_api_key_id
APP_STORE_CONNECT_ISSUER_ID=your_issuer_id
APP_STORE_CONNECT_PRIVATE_KEY_PATH=./private_keys/AuthKey_XXXXXXXXXX.p8

# Configuração do iOS
IOS_BUNDLE_IDENTIFIER=com.psiqueia.app
IOS_DEVELOPMENT_TEAM=YOUR_TEAM_ID
IOS_CODE_SIGNING_STYLE=Automatic

# Configuração do Xcode Cloud
XCODE_CLOUD_ENABLED=true
XCODE_CLOUD_WORKFLOW_ID=your_workflow_id

# Configuração de Build
BUILD_NUMBER=1
VERSION_NUMBER=1.0.0

# Configuração de Sentry (Monitoramento de Erros)
# TODO: Configurar após criar projeto no Sentry (https://sentry.io)
SENTRY_DSN=
EXPO_PUBLIC_SENTRY_DSN=

# Configuração de Push Notifications
# TODO: Configurar após obter chaves do Firebase Cloud Messaging
FCM_SERVER_KEY=
FCM_SENDER_ID=

# Configuração de Notificações
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
EMAIL_NOTIFICATION_ENABLED=false
EMAIL_RECIPIENTS=dev@psiqueia.com

# Configuração de Análise
ENABLE_SWIFTLINT=true
ENABLE_SECURITY_SCAN=true
ENABLE_PERFORMANCE_TESTS=false

# Configuração de Deploy
AUTO_DEPLOY_TO_TESTFLIGHT=false
AUTO_DEPLOY_TO_APP_STORE=false

# Configuração de Testes
RUN_UNIT_TESTS=true
RUN_INTEGRATION_TESTS=true
RUN_UI_TESTS=false

# Configuração de Cache
ENABLE_BUILD_CACHE=true
CACHE_RETENTION_DAYS=7

# Configuração de Logs
LOG_LEVEL=info
ENABLE_DEBUG_LOGS=false